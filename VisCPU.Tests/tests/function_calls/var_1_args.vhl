#include "../utils.vhl"

string(str_test_name, "VarNoArgsTest");
string(str_ret_val_fail, "Return value is not 5");

private void TestC(var arg)
{
	return arg;
}

private void TestV(var arg)
{
	var b = arg;
	return b;
}

private void VarV1ArgsTest()
{
	BeginTest(ptr_of(str_test_name), size_of(str_test_name));
	var ar = 5;
	var a = TestC(ar);
	if(a == 5)
	{
		var c = TestV(ar);
		if(c == 5)
		{
			PassTest();
			return;
		}
	}
	FailTest(ptr_of(str_ret_val_fail), size_of(str_ret_val_fail));
	return;
}

