#include "Exception.vhl"

private static string S_INDEX_OUT_OF_BOUNDS = "The Specified Index is out of bounds";


public class OutOfBoundsException : Exception
{
	public static OutOfBoundsException()
	{

	}

	public override uint GetMessage()
	{
		return &S_INDEX_OUT_OF_BOUNDS;
	}

	public override uint GetMessageLength()
	{
		return size_of(S_INDEX_OUT_OF_BOUNDS);
	}
}