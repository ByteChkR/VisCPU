#include "System/System.vhl"


private static void Main()
{
	BENCHDRV benchDev = BENCHDRV(0xFFFF4000);
	string bName = "Prime Benchmark";
	string bRes = "Found Prime: ";
	benchDev.SetName(&bName, size_of(bName));
	benchDev.StartRun();

	for(uint i = 1; i < 5000; i++)
	{
		uint isPrime = Math.IsPrime(i);
		if(isPrime)
		{
			Console.Write(&bRes, size_of(bRes));
			Console.WriteNumberLine(i);
		}
	}

	benchDev.EndRun();
}

Main();