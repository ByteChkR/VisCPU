#include "../Exception.vhl"

private static string S_BLOCK_NOT_MAPPED = "The Block is not mapped and can not be freed.";

public class BlockNotMappedException : Exception
{
	public static BlockNotMappedException()
	{
		
	}

	public override uint GetMessage()
	{
		return &S_BLOCK_NOT_MAPPED;
	}

	public override uint GetMessageLength()
	{
		return size_of(S_BLOCK_NOT_MAPPED);
	}
}