#include "../Exception.vhl"

private static string S_DEV_NOT_FOUND = "Device was not found at specified Address.";

public class DeviceNotFoundException : Exception
{
	public static DeviceNotFoundException()
	{
		
	}

	public override uint GetMessage()
	{
		return &S_DEV_NOT_FOUND;
	}

	public override uint GetMessageLength()
	{
		return size_of(S_DEV_NOT_FOUND);
	}
}
