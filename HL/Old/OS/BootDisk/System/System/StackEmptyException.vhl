#include "Exception.vhl"

private static string S_STACK_EMPTY = "The Stack is empty.";


public class StackEmptyException : Exception
{
	public static StackEmptyException()
	{

	}

	public override uint GetMessage()
	{
		return &S_STACK_EMPTY;
	}

	public override uint GetMessageLength()
	{
		return size_of(S_STACK_EMPTY);
	}
}