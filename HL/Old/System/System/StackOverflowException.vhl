#include "Exception.vhl"

private static string S_STACK_OVERFLOW = "The Stack Overflowed.";


public class StackOverflowException : Exception
{
	public static StackOverflowException()
	{

	}

	public override uint GetMessage()
	{
		return &S_STACK_OVERFLOW;
	}

	public override uint GetMessageLength()
	{
		return size_of(S_STACK_OVERFLOW);
	}
}