#include "Exception.vhl"

private static string S_PARTITION_TBL_CORRUPTED = "The Partition Table is Corrupted and can not be loaded.";


public class PartitionTableCorruptedException : Exception
{
	public static PartitionTableCorruptedException()
	{

	}

	public override uint GetMessage()
	{
		return &S_PARTITION_TBL_CORRUPTED;
	}

	public override uint GetMessageLength()
	{
		return size_of(S_PARTITION_TBL_CORRUPTED);
	}
}