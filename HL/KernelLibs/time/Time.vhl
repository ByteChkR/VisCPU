public static uint time_get_mins(uint t)
{
	return t / 60;
}

public static uint time_get_hrs(uint t)
{
	return time_get_mins(t) / 60;
}

public static uint time_get_mins_in_hour(uint t)
{
	return time_get_mins(t) % 60;
}

public static uint time_get_hour_in_day(uint t)
{
	return time_get_hrs(t) % 24;
}

public static uint time_get_sec_in_min(uint t)
{
	return t % 60;
}

